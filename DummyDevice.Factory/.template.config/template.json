{
  "$schema": "http://json.schemastore.org/template",
  "author": "Rahul Bajaj",
  "classifications": [ "CommonDevice", "Device", "Factory" ],
  "identity": "DummyDevice.Factory.Template",
  "name": "DummyDevice Factory Template",
  "shortName": "devicefactory",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "sourceName": "DummyDevice.Factory",
  "preferNameDirectory": true,
  "symbols": {
    "generalProjectName": {
      "type": "parameter",
      "datatype": "string",
      "description": "Enter the name of the general project to reference.",
      "replaces": "DummyDevice.General",
      "defaultValue": "DummyDevice.General",
      "isRequired": true
    },
    "abstractProjectName": {
      "type": "parameter",
      "datatype": "string",
      "description": "Enter the name of the abstract project to reference indirectly.",
      "replaces": "DummyDevice.Abstract",
      "defaultValue": "DummyDevice.Abstract",
      "isRequired": true
    }
  },
  "primaryOutputs": [
    {
      "path": "DummyDevice.Factory.csproj"
    }
  ],
  "defaultName": "DummyDevice.Factory",
  "description": "A template for creating a factory project that references a general device project and indirectly uses an abstract project.",
  "icon": "icon.png",
  "previewImage": "preview.png"
}
